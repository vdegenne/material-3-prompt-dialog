  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@material/web": "^1.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "lit-html": "^3.1.0",
    "prettier": "^2.8.8",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
